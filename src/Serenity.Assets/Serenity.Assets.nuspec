<?xml version="1.0" encoding="utf-8"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <id>Serenity.Assets</id>
    <version>5.0.12</version>
    <authors>${authors}</authors>
    <owners>${owners}</owners>
    <title>Serenity Platform - Web Assets</title>
    <description>Contains static assets for Serenity web applications</description>
    <language>${language}</language>
    <iconUrl>${iconUrl}</iconUrl>
    <license type="expression">MIT</license>
    <projectUrl>${projectUrl}</projectUrl>
    <copyright>${copyright}</copyright>
    <tags>${tags}</tags>
    <dependencies>
      <dependency id="jQuery" version="3.5.1" />
      <dependency id="jQuery.UI.Combined" version="1.12.1" />
      <dependency id="jQuery.Validation" version="1.19.2" />
      <dependency id="bootstrap" version="3.4.1" />
      <dependency id="toastr" version="2.1.1" />
      <dependency id="Select2.js" version="[3.5.1]" />
    </dependencies>
  </metadata>
  <files>
    <file src="Content\**\*" exclude="**\*.bak;**\*.log;**\*.orig" target="content\Content" />
    <file src="fonts\**\*" exclude="**\*.bak;**\*.log;**\*.orig" target="content\fonts" />
    <file src="Scripts\**\*" exclude="**\*.bak;**\*.log;**\*.orig" target="content\Scripts" />
  </files>
</package>