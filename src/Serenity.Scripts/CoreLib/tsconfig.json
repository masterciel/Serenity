{
  "compileOnSave": true,
  "compilerOptions": {
    "preserveConstEnums": false,
    "experimentalDecorators": true,
    "declaration": true,
    "emitBOM": true,
    "noEmit": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "module": "ES2015",
    "moduleResolution": "Node",
    "isolatedModules": true,
    "skipLibCheck": true,
    "types": [
      "jquery",
      "jquery.blockui",
      "jquery.cookie",
      "jquery.validation",
      "jqueryui",
      "bootstrap",
      "sortablejs",
      "toastr",
      "react"
    ],
    "inlineSources": true,
    "outDir": "../dist",
    "suppressImplicitAnyIndexErrors": true,
    "target": "ES5",
    "lib": [
      "ES5",
      "ES2015",
      "DOM"
    ],
    "noEmitHelpers": false,
    "jsx": "react"
  },
  "typeRoots": [
    "../node_modules/@types"
  ],
  "files": [  
    "index.ts"
  ]
}