{
  "compileOnSave": false,
  "compilerOptions": {
    "experimentalDecorators": true,
    "declaration": true,
    "declarationDir": "../../dist",
    "emitBOM": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "module": "ES2015",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "types": [
      "jquery",
      "jquery.validation",
      "jqueryui",
      "bootstrap",
      "toastr",
      "react"
    ],
    "outDir": "../../dist",
    "suppressImplicitAnyIndexErrors": true,
    "target": "ES5",
    "lib": [
      "ES5",
      "ES2015",
      "DOM"
    ],
    "noEmitHelpers": false,
    "importHelpers": false,
    "jsx": "react"
  },
  "exclude": ["**/*.spec.ts"]
}