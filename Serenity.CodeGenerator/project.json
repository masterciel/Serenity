{
  "version": "2.8.11-*",
  "description": "Serenity Tooling for .NET Core CLI. Contains the dotnet-sergen command used to generate code, transform templates, restore content packages.",
  "buildOptions": {
    "emitEntryPoint": true,
    "warningsAsErrors": true,
    "embed": {
      "mappings": {
        "typeScriptServices.js": "../Serenity.Web/Tooling/JsEngine/typescriptServices.js",
        "Serenity.CodeGeneration.js": "../Serenity.TypeScript.CodeGeneration/Serenity.CodeGeneration.js",
        "lib.d.ts_": "../Serenity.Web/Tooling/JsEngine/lib.d.ts_"
      }
    }
  },
  "dependencies": {
    "FirebirdSql.Data.FirebirdClient": "5.7.0",
    "Microsoft.Data.Sqlite": "1.1.0",
    "MySqlConnector": "0.11.3",
    "Newtonsoft.Json": "9.0.1",
    "Npgsql": "3.1.9",
    "Serenity.Web": "2.8.11-*"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "netstandard1.6",
        "netstandard1.3"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        }
      }
    }
  }
}